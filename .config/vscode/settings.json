{
  "window.commandCenter": true,
  "workbench.preferredDarkColorTheme": "Quiet Light",
  "keyboard.touchbar.enabled": false,
  "files.autoSave": "afterDelay",
  "window.titleBarStyle": "custom",
  "window.title": "${separator}${activeRepositoryName}${separator}${activeRepositoryBranchName}",
  "editor.cursorStyle": "block-outline",
  "workbench.colorTheme": "Default Dark Modern",
  "diffEditor.ignoreTrimWhitespace": false,
  "vscode-dataform-tools.enableLogging": true,
  "vscode-dataform-tools.gcpProjectId": "dragonfly-data-0001",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "[sqlx]": {
    "editor.defaultFormatter": "ashishalex.dataform-lsp-vscode",
    "editor.formatOnSave": true
  },
  "[dataform]": {
    "editor.defaultFormatter": "ashishalex.dataform-lsp-vscode",
    "editor.formatOnSave": true
  },
  "vscode-dataform-tools.compileAndDryRunBeforeFormatting": false,
  "git.autofetch": true,
  "go.toolsManagement.autoUpdate": true,
  "githubPullRequests.fileListLayout": "tree",
  "git.confirmSync": false,
  "files.exclude": {
    ".idea": true,
    "node_modules": true
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true
  },
  "vscode-dataform-tools.currencyFoDryRunCost": "GBP",
  "vscode-dataform-tools.formattingCli": "dataform",
  "makefile.configureOnOpen": false,
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.pullBranch": "never",
  "claudeCode.allowDangerouslySkipPermissions": true,
  "claudeCode.environmentVariables": [
    {
      "name": "DISABLE_PROMPT_CACHING",
      "value": "0"
    },
    {
      "name": "CLAUDE_CODE_USE_BEDROCK",
      "value": "1"
    },
    {
      "name": "AWS_PROFILE",
      "value": "bedrock-access-316332150940"
    },
    {
      "name": "AWS_REGION",
      "value": "us-east-1"
    },
    {
      "name": "CLAUDE_CODE_SKIP_AUTH_LOGIN",
      "value": "1"
    }
  ],
  "claudeCode.provider": "bedrock",
  "claudeCode.initialPermissionMode": "bypassPermissions",
  "claudeCode.preferredLocation": "panel",
  "claudeCode.selectedModel": "us.anthropic.claude-opus-4-6-v1",
  "editor.accessibilitySupport": "on",
